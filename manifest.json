{
    "manifest_version": 3,
    "name": "SmartRevise ChatGPT Helper",
    "version": "1.0",
    "description": "A Chrome extension to use ChatGPT to answer questions on SmartRevise.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://*.smartrevise.online/*"],
    "content_scripts": [
      {
        "matches": ["*://*.smartrevise.online/*"],
        "js": ["jquery.min.js", "content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  